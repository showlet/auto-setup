set encoding=utf-8 " encoding used for displaying file
set nocompatible
set wildmenu
set showcmd
set hlsearch
set ignorecase
set smartcase
set backspace=indent,eol,start
set autoindent
set nostartofline
set ruler
set laststatus=2
set mouse=a
set cmdheight=2
set relativenumber
set shiftwidth=2
set tabstop=2
set softtabstop=2
set expandtab
map Y y$

syntax on

